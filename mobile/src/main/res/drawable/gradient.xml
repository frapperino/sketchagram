<?xml version="1.0" encoding="UTF-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape
            android:shape="rectangle">
            <solid
                android:color="@color/sketchagramSecondary2"/>
        </shape>
    </item>
    <item android:state_pressed="false">
        <shape
            android:shape="rectangle">
            <solid
                android:color="@color/sketchagramSecondary"/>
        </shape>
    </item>
    <item android:state_focused="true">
        <shape
            android:shape="rectangle">
            <solid
                android:color="@color/sketchagramSecondary"/>
            <stroke android:width="2dp" android:color="@color/sketchagramSecondary2"></stroke>
        </shape>
    </item>
</selector>